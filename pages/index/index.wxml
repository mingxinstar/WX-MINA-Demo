<!--index.wxml-->
<view class="container">
    <view class="star-list">
        <block wx:for="{{liveList}}">
            <view bindtap="gotoLivePage" class="star-item" data-id="{{item._id}}">
                <image src="{{item.pic}}"></image>
                <div class="star-name">{{item.nick_name}}</div>
            </view>
        </block>
    </view>
</view>
